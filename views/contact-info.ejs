<div class="header">Contact Details</div>

<div class="form-group">
	<label for="fullname">Phone Number</label> <input type="text"
		class="form-control" id="qualification" name="qualification"
		placeholder="Enter educational qualification" required
		value='<%=locals.user!=undefined? user.phonenumber:""%>'>
</div>
<div class="form-group">
	<div class="col-sm-12 col-md-12">
		<div class="checkbox pull-left">
			<label><input type="checkbox" tabindex='5' required
				name="hidePhonenumber" class="pull-left"> Hide phone Number</label>
		</div>
	</div>
</div>
<div class="form-group">
	<label for="dob">Email Id</label> <input type="email"
		class="form-control" id="email" name="email"
		value='<%=locals.user!=undefined? user.email:""%>' placeholder="Email">
</div>
 

