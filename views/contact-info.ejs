<div class="header">Contact Details</div>



<div class="form-group">
	<label for="fullname">Phone Number</label> <input type="text"
		class="form-control" id="phonenumber" name="phonenumber"
		placeholder="Enter phone number" required-field tabindex='1' parent-div="page4"
		desc="Phone number"
		value='<%=locals.user!=undefined? user.phonenumber:""%>'>
</div>
<div class="form-group">
 
		<div class="checkbox hide-phonenumber-chkbox">
			<label><input type="checkbox" tabindex='2'  
				name="hidephonenumber" class="pull-left"
				id="hidephonenumber"
				>Hide phone Number</label>
		</div>
	 
</div>
<div class="form-group">
	<label for="dob">Email Id</label> <input type="email" required-field
	    desc="email"
		class="form-control" id="email" name="email" tabindex='3' parent-div="page4"
		value='<%=locals.user!=undefined? user.email:""%>' placeholder="Email">
</div>
 

